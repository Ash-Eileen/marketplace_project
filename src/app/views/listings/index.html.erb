<p id="notice"><%= notice %></p>

<h1>Listings</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Bio</th>
      <th>Agent</th>
      <th>Availability</th>
      <th>Price</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @listings.each do |listing| %>
      <tr>
        <td><%= listing.seller.username %></td>
        <td><%= listing.bio %></td>
        <td><%= listing.agent %></td>
        <td><%= listing.availability %></td>
        <td><%= number_to_currency(listing.price) %></td>
        <td> <%= image_tag listing.picture if listing.picture.attached? %> </td>
        <td><%= link_to 'Show', listing %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Listing', new_listing_path %>
